module(name = "main", version="")

bazel_dep(name = "rules_java", version = "8.16.1")
bazel_dep(name = "rules_jvm_external", version = "6.8")
# -- bazel_dep definitions -- #

# -- use_repo_rule statements -- #


maven = use_extension("@rules_jvm_external//:extensions.bzl", "maven")

use_repo(maven, "px_deps")
maven.install(
	name = "px_deps",
    artifacts = [
        "org.antlr:antlr4:4.11.1",
		"com.google.jimfs:jimfs:1.2",
		"com.google.truth:truth:1.4.0",
    ],
    repositories = [
        "https://repo1.maven.org/maven2",
    ],
)

# -- End of maven artifacts for repo `px_deps` -- #

use_repo(maven, "maven")
maven.artifact(
    testonly = True,
    group = "biz.aQute",
    artifact = "bnd",
    version = "0.0.384"
)

maven.artifact(
    testonly = True,
    group = "com.google.truth",
    artifact = "truth",
    version = "1.4.0"
)

maven.install(
    artifacts = [
        "aopalliance:aopalliance:1.0",
		"org.apache.santuario:xmlsec:2.2.0",
    ],
    repositories = [
        "https://maven.google.com",
		"https://repo1.maven.org/maven2",
    ],
)

# -- End of maven artifacts for repo `maven` -- #
