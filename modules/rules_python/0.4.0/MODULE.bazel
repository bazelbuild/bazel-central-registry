module(
  name = "rules_python",
  version = "0.4.0",
  compatibility_level = 1,
)

pip_install = use_extension("@rules_python//bzlmod:extensions.bzl", "pip_install")

use_repo(pip_install,
  "pypi__click",
  "pypi__pip",
  "pypi__pip_tools",
  "pypi__pkginfo",
  "pypi__setuptools",
  "pypi__wheel",
)
register_toolchains("@bazel_tools//tools/python:autodetecting_toolchain")
