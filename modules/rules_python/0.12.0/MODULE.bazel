module(
  name = "rules_python",
  version = "0.12.0",
  compatibility_level = 1,
  toolchains_to_register = ["@bazel_tools//tools/python:autodetecting_toolchain"],
)

pip_install = use_extension("//python:extensions.bzl", "pip_install")

use_repo(pip_install,
  "pypi__build",
  "pypi__click",
  "pypi__colorama",
  "pypi__importlib_metadata",
  "pypi__installer",
  "pypi__more_itertools",
  "pypi__packaging",
  "pypi__pep517",
  "pypi__pip",
  "pypi__pip_tools",
  "pypi__pyparsing",
  "pypi__setuptools",
  "pypi__tomli",
  "pypi__wheel",
  "pypi__zipp",
)
