module(
    name = "bazel_central_registry",
    compatibility_level = 1,
    version = "0.0.0",
)

bazel_dep(name = "rules_python", version = "0.10.2")

pip_install = use_extension("@rules_python//python:extensions.bzl", "pip_install")

use_repo(pip_install,
  "pypi__click",
  "pypi__pep517",
  "pypi__pip",
  "pypi__pip_tools",
  "pypi__setuptools",
  "pypi__tomli",
  "pypi__wheel",
)
